#breadcrumbs {
    font-size: clamp(11px, 1.2vw,16px);
}
#breadcrumbs ul {
    display: flex;
    list-style: none;
    margin: 2%;
}
#breadcrumbs ul li a {
    font-weight: bold !important;
    color: #1B365D !important;
    text-decoration: underline !important;
}
#breadcrumbs ul li:not(:last-child):after {
    content: ">";
    color: #1B365D;
    padding-left: 10px;
    padding-right: 10px;
}
@media screen and (max-width: 992px){
    #breadcrumbs ul{
        display: block;
    }
}