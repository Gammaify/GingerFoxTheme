.nextArrow{
    width:20px;
    height: 20px;
    transform: scaleX(-1);
    margin-left: 10px;
}
.prevArrow{
    width:20px;
    height: 20px;
    margin-right: 10px;
}
.PageNumbers {
    display: flex;
    font-size: 20px;
    font-weight: bold;
    align-items: center;
}
#currentPage {
    border-color: #FF8200 !important;
    margin-right: 3px;
    width: 50px;
    border-radius: 10px;
    text-align: center;
    -moz-appearance: textfield;
}

.pagination{
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 10px;
    margin-bottom: 10px;
}