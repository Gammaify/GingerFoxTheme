#main {
    overflow: hidden;
}

body {
    background-color: #f1eedf;
}

.GreenColorSectionTitles {
    color: #207231;
}

.GreenColor {
    color: #276023;
}

.fontWeight300 {
    font-weight: 300;
}

.fontWeight500 {
    font-weight: 500;
    font-size: 15px;
}

.OffWhiteText {
    color: rgb(239, 233, 215);
}

.marginTop {
    margin-top: 10rem;
}

@media screen and (max-width: 768px) {
    .marginTop {
        margin-top: 0rem;
    }
}

.row {
    --bs-gutter-x: unset !important;
    --bs-gutter-y: unset !important;
}


.titlePosition {
    top: 45px;
    left: 90px;
}

.titleText {
    font-size: 8vw;
}

.roundRight {
    border: 2px solid #91C038;
    background-color: #91C038;
    padding: 10px;
    border-top-right-radius: 50px 50px;
    border-bottom-right-radius: 50px 50px;
    min-height: 80px;
    position: relative;
}

.roundLeft {
    border: 2px solid #91C038;
    background-color: #91C038;
    padding: 10px;
    border-top-Left-radius: 50px 50px;
    border-bottom-left-radius: 50px 50px;
    min-height: 80px;
    position: relative;
}

.imageLeft {
    position: absolute;
    left: 10%;
    top: -40px;
    margin: auto;
}

.imageRight {
    position: absolute;
    left: 75%;
    top: -40px;
    margin: auto;
}

.paddingLeft {
    padding: 0 3% 0 45%;
}

.paddingRight {
    padding: 0 45% 0 3%;
}

.paddingAll {
    padding: 40px 80px 40px 80px;
}

.samerowOffsetMargin {
    margin-top: 70px;
}


.short-border {
    width: 250px;
    border-top: solid 1px #91c038;
    margin: 22px auto 20px;
    height: 1px;
}

.centreTextInDiv {
    padding-top: 15px;
}

.sustainabilityTitle h1 {
    position: absolute;
    color: #efe9d7;
    width: 100%;
    top: 50%;
    font-size: clamp(15px, 4.2vw, 32px);
    text-align: center;
    font-weight: bold;
    text-transform: uppercase;
}

.sustainabilityTitle {
    position: relative;
    width: 700px;
    margin: auto;
    display: block;
}

.sustainabilityTitle img {
    width: 100%;
}