.videoTitleContainer{
    display: flex;
    justify-content: center;
    min-width: 300px;
    padding-top: 15px;
    padding-bottom: 15px;
}

.videoTitleContainer img{
    width: 30px;
    height: 30px;
}

.videoTitleContainer h1{
    font-size: 50px;
    font-weight: bold;
    text-transform: uppercase;
}

.auto-resizable-iframe {
    max-width: 100%;
    margin: 0px auto;
}

.auto-resizable-iframe > div {
    position: relative;
    padding-bottom: 50%;
    height: 0px;
}

.auto-resizable-iframe div iframe {
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
}